.swiper_wrap swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper_wrap swiper image {
  width: 100%;
}
.catitems_wrap {
  display: flex;
}
.catitems_wrap .catitems {
  flex: 1;
  padding: 20rpx;
}
.catitems_wrap .catitems image {
  width: 100%;
}
.floor_wrap .floor .floor_list {
  overflow: hidden;
}
.floor_wrap .floor .floor_list navigator {
  float: left;
  width: 33.33%;
  /* 后四个超链接 */
  /* 2 3 两个超链接 */
}
.floor_wrap .floor .floor_list navigator:nth-last-child(-n+4) {
  /* 原图的宽高 232 *386 */
  /*
                    232 / 386 = 33.33vw / height
                    第一张图片的高度 height:33.33vw * 386 /  232
                */
  height: calc(33.33vw * 386 / 232 / 2);
  border-left: 10rpx solid #fff;
}
.floor_wrap .floor .floor_list navigator:nth-child(2),
.floor_wrap .floor .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_wrap .floor .floor_list navigator image {
  width: 100%;
  height: 100%;
}
